  "main": "index.js",
  "scripts": {
    "prebuild": "npm-run-all clean",
    "build": "webpack",
    "clean": "rimraf dist/*",
    "watch:webextension": "web-ext --browser-console --config=web-ext-config.js run --target chromium",
    "watch:js": "webpack --watch --progress",
    "watch": "npm-run-all --parallel watch:*",
    "pregenerate": "run-s build",
    "generate": "web-ext --config=web-ext-config.js build",
    "lint": "web-ext --config=web-ext-config.js lint",
    "format": "prettier --write 'src/**/{*.js,*.json}'",
    "test": "jest"
  },
